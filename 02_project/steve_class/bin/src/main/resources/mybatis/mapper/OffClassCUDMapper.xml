<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  

<mapper namespace="offclasscudmapper">


	<insert id="offinsert" parameterType="map">
		INSERT INTO `steve_class`.`class_offline` (`class_name`, `class_amount`, `month`, `create_dt`) 
		VALUES (#{class_name}, #{class_amount}, #{month}, #{create_dt});
	</insert>
	




</mapper>
